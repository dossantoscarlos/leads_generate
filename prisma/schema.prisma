generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Lead {
  id        Int    @id @default(autoincrement())
  firstname String
  lastname  String
  zipcode   String
  street    String
  city      String
  state     String
  phone     String
  email     String
}
